{
  "name": "motor-cms/motor-cms",
  "description": "Motor content management module",
  "license": "MIT",
  "keywords": [
    "motor"
  ],
  "authors": [
    {
      "name": "Reza Esmaili",
      "email": "me@dfox.info"
    }
  ],
  "require": {
    "php": "^8.2",
    "motor-cms/motor-backend": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "Motor\\CMS\\": "src",
      "Tests\\Motor\\CMS\\": "tests",
      "Motor\\CMS\\Database\\Factories\\": "database/factories/",
      "Motor\\CMS\\Database\\Seeders\\": "database/seeders/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Motor\\CMS\\Providers\\MotorServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable" : true
}
